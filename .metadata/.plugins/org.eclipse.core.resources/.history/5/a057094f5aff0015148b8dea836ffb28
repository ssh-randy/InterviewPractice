package listStuff;

public class LinkedListRecursive<T> implements List<T> {

	private ListCell<T> head;
	
	public LinkedListRecursive(){
		head = null;
	}
	
	public void insert(T element) {
		head = new ListCell<T>(element, head);
	}

	public void delete(T element) {
		delete(element, head);
		
	}
	
